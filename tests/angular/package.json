{
	"name": "angular",
	"version": "0.0.0",
	"scripts": {
		"start": "ng serve",
		"build": "ng build",
		"serve-dist": "cd dist/angular/browser && http-server",
		"uglify": "node ../../bin/uglify-css.js",
		"compress": "node ../test-zip.js -d=dist/angular/browser -n=$SUFFIX",
		"compress:before": "SUFFIX=default npm run compress",
		"compress:after": "SUFFIX=uglified npm run compress",
		"test": "npm run build && npm run compress:before && npm run uglify -- -d -a=length && npm run uglify -- -d -a=count && npm run uglify -- -d -a=countXlength && npm run uglify && npm run compress:after && ls -la dist/angular/browser/index.html*.br"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "^19.0.0",
		"@angular/common": "^19.0.0",
		"@angular/compiler": "^19.0.0",
		"@angular/core": "^19.0.0",
		"@angular/forms": "^19.0.0",
		"@angular/platform-browser": "^19.0.0",
		"@angular/platform-browser-dynamic": "^19.0.0",
		"@angular/router": "^19.0.0",
		"@tailwindcss/postcss": "^4.1.7",
		"postcss": "^8.5.3",
		"rxjs": "~7.8.0",
		"tailwindcss": "^4.1.7",
		"tslib": "^2.3.0",
		"zone.js": "~0.15.0"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "^19.0.3",
		"@angular/cli": "^19.0.3",
		"@angular/compiler-cli": "^19.0.0",
		"@types/jasmine": "~5.1.0",
		"jasmine-core": "~5.4.0",
		"karma": "~6.4.0",
		"karma-chrome-launcher": "~3.2.0",
		"karma-coverage": "~2.2.0",
		"karma-jasmine": "~5.1.0",
		"karma-jasmine-html-reporter": "~2.1.0",
		"typescript": "~5.6.2"
	}
}
