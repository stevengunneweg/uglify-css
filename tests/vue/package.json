{
	"name": "uglify-css",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"start": "vite",
		"build": "vite build",
		"serve-dist": "cd dist && http-server",
		"uglify": "node ../../bin/uglify-css.js",
		"compress": "node ../test-zip.js -d=dist -n=$SUFFIX",
		"compress:before": "SUFFIX=default npm run compress",
		"compress:after": "SUFFIX=uglified npm run compress",
		"test": "npm run build && npm run compress:before && npm run uglify -- -d -a=length && npm run uglify -- -d -a=count && npm run uglify -- -d -a=countXlength && npm run uglify && npm run compress:after && ls -la dist/index.html*.br"
	},
	"dependencies": {
		"vue": "^3.5.13"
	},
	"devDependencies": {
		"@tailwindcss/vite": "^4.1.7",
		"@vitejs/plugin-vue": "^5.2.3",
		"prettier": "3.5.3",
		"prettier-plugin-tailwindcss": "0.6.11",
		"tailwindcss": "^4.1.7",
		"vite": "^6.3.5"
	}
}
